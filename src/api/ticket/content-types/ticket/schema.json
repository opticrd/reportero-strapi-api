{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "singularName": "ticket",
    "pluralName": "tickets",
    "displayName": "Ticket",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service"
    },
    "institution": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::institution.institution",
      "inversedBy": "tickets"
    },
    "beneficiary": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::beneficiary.beneficiary",
      "inversedBy": "tickets"
    },
    "address": {
      "type": "text",
      "required": true
    },
    "evidence": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "text"
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "tickets"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "ticket"
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "new",
        "open",
        "pending_reminder",
        "closed",
        "merged",
        "removed",
        "pending_close"
      ],
      "default": "new",
      "required": true
    },
    "priority": {
      "type": "enumeration",
      "enum": [
        "low",
        "normal",
        "high"
      ],
      "default": "normal",
      "required": true
    },
    "blocked": {
      "type": "boolean",
      "default": false
    },
    "blocked_comments": {
      "type": "boolean",
      "default": false
    },
    "block_reason": {
      "type": "text"
    },
    "reports": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::report.report",
      "mappedBy": "ticket"
    },
    "zone_code": {
      "type": "string",
      "required": true
    },
    "snip_code": {
      "type": "string"
    }
  }
}
